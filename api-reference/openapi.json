        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["agent_id", "phone_number"],
                "properties": {
                  "agent_id": {
                    "type": "integer",
                    "description": "ID of the agent to attach phone numbers to"
                  },
                  "phone_number": {
                    "type": "array",
                    "description": "Phone numbers to attach to the agent. Must include country code with + prefix (e.g., '+1234567890')"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": { 
            "description": "Numbers attached successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": { "type": "boolean" },
                    "message": { "type": "string" },
                    "data": {
                      "type": "object",
                      "properties": {
                        "agent_id": { "type": "integer" },
                        "phone_numbers": { "type": "array", "items": { "type": "string" } }
                      }
                    }
                  }
                }
              }
            }
          },
          "400": { "description": "Bad Request - Phone Number or Agent ID is required" },
          "401": { "description": "Unauthorized" },
          "404": { "description": "Agent not found or not active" },
          "422": { "description": "Validation error" }
        }
      }
    },
    "/global/agents": {
      "post": {
        "summary": "Create voice agent",
        "tags": ["Voice AI"],
        "security": [],
        "parameters": [
          {
            "name": "token",
            "in": "header",
            "required": true,
            "description": "API token for authentication",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["agent_name", "agent_prompt"],
                "properties": {
                  "agent_name": {
                    "type": "string",
                    "description": "Name of the voice agent"
                  },
                  "agent_prompt": {
                    "type": "string",
                    "description": "System prompt/instructions for the agent"
                  },
                  "description": {
                    "type": "string",
                    "description": "Description of the agent"
                  },
                  "voice_id": {
                    "type": "string",
                    "description": "Voice ID to use for the agent. Defaults to 'RXe6OFmxoC0nlSWpuCDy' if not provided"
                  },
                  "voice_provider": {
                    "type": "integer",
                    "description": "Voice provider ID (1=ElevenLabs, 2=OpenAI, 3=Deepgram, 4=Sarvam). Defaults to 1"
                  },
                  "transcriber_provider": {
                    "type": "integer",
                    "description": "Speech-to-text provider ID. Defaults to 2 (Deepgram)"
                  },
                  "transcriber_language": {
                    "type": "integer",
                    "description": "Language ID for transcription. Defaults to 1 (English)"
                  },
                  "transcriber_model": {
                    "type": "integer",
                    "description": "Transcriber model ID. Defaults to 16 (nova-3) when using Deepgram"
                  },
                  "calling_type": {
                    "type": "integer",
                    "description": "Calling type (1=Outbound, 2=Inbound). Defaults to 1"
                  },
                  "purpose": {
                    "type": "integer",
                    "description": "Purpose ID for the agent (1-15). See Purpose Options in documentation"
                  },
                  "tts_model": {
                    "type": "string",
                    "description": "TTS model for voice synthesis. Defaults to 'eleven_multilingual_v2'"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": { "description": "Voice agent created successfully" },
          "400": { "description": "Bad Request - Agent Name is required" },
          "401": { "description": "Unauthorized" },
          "422": { "description": "Validation error" }
        }
      }
    },
