    "/global/agents/update": {
      "post": {
        "summary": "Update voice agent",
        "tags": ["Voice AI"],
        "security": [],
        "parameters": [
          {
            "name": "token",
            "in": "header",
            "required": true,
            "description": "API token for authentication",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "agent_id",
            "in": "query",
            "required": false,
            "description": "ID of the agent to update (can also be provided in request body)",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["agent_id"],
                "properties": {
                  "agent_id": {
                    "type": "integer",
                    "description": "ID of the agent to update"
                  },
                  "agent_name": {
                    "type": "string",
                    "description": "Updated name of the agent"
                  },
                  "description": {
                    "type": "string",
                    "description": "Updated description"
                  },
                  "agent_prompt": {
                    "type": "string",
                    "description": "Updated system prompt/instructions"
                  },
                  "voice_id": {
                    "type": "string",
                    "description": "Updated voice ID (string format)"
                  },
                  "voice_id_json": {
                    "type": "string",
                    "description": "Updated voice ID JSON containing complete voice metadata"
                  },
                  "voice_provider": {
                    "type": "integer",
                    "description": "Updated voice provider ID"
                  },
                  "transcriber_provider": {
                    "type": "integer",
                    "description": "Updated speech-to-text provider ID"
                  },
                  "transcriber_language": {
                    "type": "integer",
                    "description": "Updated language ID"
                  },
                  "transcriber_model": {
                    "type": "integer",
                    "description": "Updated transcriber model ID"
                  },
                  "calling_type": {
                    "type": "integer",
                    "description": "Updated calling type (1=Outbound, 2=Inbound)"
                  },
                  "purpose": {
                    "type": "integer",
                    "description": "Updated purpose ID"
                  },
                  "tts_model": {
                    "type": "string",
                    "description": "Updated TTS model for voice synthesis"
                  },
                  "status": {
                    "type": "integer",
                    "description": "Updated agent status (1=Active, 3=Draft)"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": { "description": "Voice agent updated successfully" },
          "400": { "description": "Bad Request - Agent ID is required" },
          "401": { "description": "Unauthorized" },
          "404": { "description": "Agent not found" },
          "422": { "description": "Validation error" }
        }
      }
    },
    "/global/agents/delete": {
      "post": {
