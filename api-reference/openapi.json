        "requestBody": {
          "required": false,
          "content": {
            "application/json": {
              "schema": {
                "type": "object"
              }
            }
          }
        },
        "responses": {
          "200": { "description": "API token generated successfully" },
          "400": { "description": "LOGIN_TOKEN_REQUIRED" },
          "401": { "description": "INVALID_LOGIN_TOKEN or SESSIONEXPIRED" }
